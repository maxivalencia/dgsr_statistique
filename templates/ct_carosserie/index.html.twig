{% extends 'base.html.twig' %}

{% block title %}CtCarosserie index{% endblock %}

{% block body %}
    <h1>CtCarosserie index</h1>

    <table id="user_liste" class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>CrsLibelle</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for ct_carosserie in ct_carosseries %}
            <tr>
                <td>{{ ct_carosserie.id }}</td>
                <td>{{ ct_carosserie.crsLibelle }}</td>
                <td>
                    <a href="{{ path('ct_carosserie_show', {'id': ct_carosserie.id}) }}">show</a>
