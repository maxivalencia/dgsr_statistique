{% extends 'base.html.twig' %}

{% block title %}CtUsage{% endblock %}

{% block body %}
    <h1>Usage</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ ct_usage.id }}</td>
            </tr>
            <tr>
                <th>Usage Libelle</th>
                <td>{{ ct_usage.usgLibelle }}</td>
            </tr>
            <tr>
                <th>Usage Validite</th>
                <td>{{ ct_usage.usgValidite }}</td>
            </tr>
            <tr>
                <th>UsgCreated</th>
                <td>{{ ct_usage.usgCreated ? ct_usage.usgCreated|date('d/m/Y H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('ct_usage_index') }}">Retour</a>

    <a href="{{ path('ct_usage_edit', {'id': ct_usage.id}) }}">Modifier</a>

    {{ include('ct_usage/_delete_form.html.twig') }}
{% endblock %}
