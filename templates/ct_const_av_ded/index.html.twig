{% extends 'base.html.twig' %}

{% block title %}CtConstAvDed index{% endblock %}

{% block body %}
    <h1>CtConstAvDed index</h1>

    <table id="user_liste" class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>CadProvenance</th>
                <th>CadDivers</th>
                <th>CadProprietaireNom</th>
                <th>CadProprietaireAdresse</th>
                <th>CadBonEtat</th>
                <th>CadSecPers</th>
                <th>CadSecMarch</th>
                <th>CadProtecEnv</th>
                <th>CadNumero</th>
                <th>CadImmatriculation</th>
                <th>CadDateEmbarquement</th>
                <th>CadLieuEmbarquement</th>
                <th>CadCreated</th>
                <th>CadConforme</th>
                <th>CadObservation</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for ct_const_av_ded in ct_const_av_deds %}
            <tr>
                <td>{{ ct_const_av_ded.id }}</td>
                <td>{{ ct_const_av_ded.cadProvenance }}</td>
                <td>{{ ct_const_av_ded.cadDivers }}</td>
                <td>{{ ct_const_av_ded.cadProprietaireNom }}</td>
                <td>{{ ct_const_av_ded.cadProprietaireAdresse }}</td>
                <td>{{ ct_const_av_ded.cadBonEtat ? 'Yes' : 'No' }}</td>
                <td>{{ ct_const_av_ded.cadSecPers ? 'Yes' : 'No' }}</td>
                <td>{{ ct_const_av_ded.cadSecMarch ? 'Yes' : 'No' }}</td>
                <td>{{ ct_const_av_ded.cadProtecEnv ? 'Yes' : 'No' }}</td>
                <td>{{ ct_const_av_ded.cadNumero }}</td>
                <td>{{ ct_const_av_ded.cadImmatriculation }}</td>
                <td>{{ ct_const_av_ded.cadDateEmbarquement ? ct_const_av_ded.cadDateEmbarquement|date('Y-m-d H:i:s') : '' }}</td>
