                <td>{{ ct_const_av_ded.cadObservation }}</td>
                <td>
                    <a href="{{ path('ct_const_av_ded_show', {'id': ct_const_av_ded.id}) }}">show</a>
                    <a href="{{ path('ct_const_av_ded_edit', {'id': ct_const_av_ded.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="17">no records found</td>
