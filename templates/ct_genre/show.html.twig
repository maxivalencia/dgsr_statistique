{% extends 'base.html.twig' %}

{% block title %}CtGenre{% endblock %}

{% block body %}
    <h1>Genre</h1>
    <div  align="center"><a href="{{ path('acceuil') }}" class="btn btn-success"><i class="fa fa-book" aria-hidden="true"> Acceuil</i></a></div>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ ct_genre.id }}</td>
            </tr>
            <tr>
                <th>Genre Libelle</th>
                <td>{{ ct_genre.grLibelle }}</td>
            </tr>
            <tr>
                <th>Genre Code</th>
                <td>{{ ct_genre.grCode }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('ct_genre_index') }}">Retour</a>

    <a href="{{ path('ct_genre_edit', {'id': ct_genre.id}) }}">Modifier</a>

    {{ include('ct_genre/_delete_form.html.twig') }}
