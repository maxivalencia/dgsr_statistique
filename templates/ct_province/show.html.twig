{% extends 'base.html.twig' %}

{% block title %}CtProvince{% endblock %}

{% block body %}
    <h1>Province</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ ct_province.id }}</td>
            </tr>
            <tr>
                <th>Province Nom</th>
                <td>{{ ct_province.prvNom }}</td>
            </tr>
            <tr>
                <th>Province Code</th>
                <td>{{ ct_province.prvCode }}</td>
            </tr>
            <tr>
                <th>Province CreatedAt</th>
                <td>{{ ct_province.prvCreatedAt ? ct_province.prvCreatedAt|date('d/m/Y H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>PrvUpdatedAt</th>
                <td>{{ ct_province.prvUpdatedAt ? ct_province.prvUpdatedAt|date('d/m/Y H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('ct_province_index') }}">Retour</a>

    <a href="{{ path('ct_province_edit', {'id': ct_province.id}) }}">Modifier</a>

    {{ include('ct_province/_delete_form.html.twig') }}
{% endblock %}
